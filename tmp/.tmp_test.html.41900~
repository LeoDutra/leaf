		<style type="text/css" >
			
			.doo {
				width: 40px;
				height: 40px;
				position: absolute;
				top: 10px;
				left: 10px;
				background-color: black;
				border: 1px solid red;
			}
			
		</style>
		<script type="text/javascript" src="leaf 0.6a.js"></script>
		<script type="text/javascript">
			

			function block() {
				var arrCheck = document.getElementsByName('check');
				var arrIdentificador = document.getElementsByName('identificador');
				var arrDataPrevisaoPagamento = document.getElementsByName('dataPrevisaoPagamento');
				var arrNovaDataPrevisaoPagamento = document.getElementsByName('novaDataPrevisaoPagamento');
				var arrNovaDataPrevisaoPagamentoTemp = document.getElementsByName('novaDataPrevisaoPagamentoTemp');
				
				/* define caches */
				var i;
				var item1;
				var item2;
				/* end define caches */
				
				if ((i = arrNovaDataPrevisaoPagamentoTemp.length)){
					while (i--) {
						arrNovaDataPrevisaoPagamentoTemp[i].disabled = true;
					}
				}
				
				if ((i = arrDataPrevisaoPagamento.length)){
					while (i--) {
						if (!(item1 = arrCheck[i]).checked ){
							item1.disabled = true;
						} 
						if ((item1 = arrDataPrevisaoPagamento[i]).value === (item2 = arrNovaDataPrevisaoPagamento[i]).value){
							arrDataPrevisaoPagamento[i].disabled = (item1.disabled = (item2.disabled = true));
						}	
					}
				} else {
					i = arrCheck.length;
					while(i--){
						if (!(item1 = arrCheck[i]).checked ){
							item1.disabled = true;
						} 
						arrNovaDataPrevisaoPagamento[i].disabled = (arrIdentificador[i].disabled = true);
					}
				}	
			}
		
			window.onload = function () {
				var t0 = (new Date()).getTime();
				var i = 10000;
				while(i--) {
					var elements = document.getElementsByName('three');
				}
				alert((new Date()).getTime()-t0);
			}
			
		</script>
	</head>
